<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
         
         <servlet>
         <servlet-name>pLogin</servlet-name>
         <servlet-class>com.pat.cusLoginServlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         <servlet-name>pLogin</servlet-name>
         <url-pattern>/log</url-pattern>
         </servlet-mapping>
         
         <servlet>
         <servlet-name>E-channeling</servlet-name>
         <servlet-class>com.pat.EchannelingServlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         <servlet-name>E-channeling</servlet-name>
         <url-pattern>/E-channeling</url-pattern>
         </servlet-mapping>
         
         <servlet>
         <servlet-name>E-channelingTime</servlet-name>
         <servlet-class>com.pat.EchannelingTimeServlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         <servlet-name>E-channelingTime</servlet-name>
         <url-pattern>/EchanTime</url-pattern>
         </servlet-mapping>
         
         <servlet>
         <servlet-name>update</servlet-name>
         <servlet-class>com.pat.updateE_channeling</servlet-class>
         </servlet>
         
         <servlet-mapping>
         <servlet-name>update</servlet-name>
         <url-pattern>/update</url-pattern>
         </servlet-mapping>
         
         <servlet>
         <servlet-name>delete</servlet-name>
         <servlet-class>com.pat.deleteE_channeling</servlet-class>
         </servlet>
         
         <servlet-mapping>
         <servlet-name>delete</servlet-name>
         <url-pattern>/delete</url-pattern>
         </servlet-mapping>
         
		 <servlet>
         <servlet-name>pay</servlet-name>
         <servlet-class>com.pat.paymentServlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         <servlet-name>pay</servlet-name>
         <url-pattern>/payment</url-pattern>
         </servlet-mapping>
</web-app>



         
